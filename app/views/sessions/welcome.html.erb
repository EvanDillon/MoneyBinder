<h1>Welcome</h1>
<h1>Login</h1>
  <%= form_tag login_path do %>
  <%= label_tag :username%>
  <%= text_field_tag :username %><br>
  <%= label_tag :password%>
  <%= password_field_tag :password%><br>
  <%= submit_tag "Submit"%>
<%end%>

<%= button_to "Create new User", '/users/new', method: :get%>
<%= link_to 'Forgotten password?', password_reset_path, data: {icon: 'info', mini: 'true', role: 'button'} %>
